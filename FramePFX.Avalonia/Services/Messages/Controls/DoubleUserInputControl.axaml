<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400"
             x:Class="FramePFX.Avalonia.Services.Messages.Controls.DoubleUserInputControl"
             Focusable="False">
    <StackPanel Spacing="5" Focusable="False">
        <TextBlock x:Name="PART_LabelA" Text="Label Here" Padding="0,5"/>
        <TextBox x:Name="PART_TextBoxA" Padding="5,5" VerticalContentAlignment="Center"
                 Background="{DynamicResource ABrush.Tone2.Border.Static}"
                 Text="Text Here" Focusable="True" TabIndex="0"/>
        <TextBlock x:Name="PART_LabelB" Text="Label Here" Padding="0,5"/>
        <TextBox x:Name="PART_TextBoxB" Padding="5,5" VerticalContentAlignment="Center"
                 Background="{DynamicResource ABrush.Tone2.Border.Static}"
                 Text="Text Here" Focusable="True" TabIndex="1"/>
    </StackPanel>
</UserControl>